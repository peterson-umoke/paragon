{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "akaunting/laravel-money": "^1.2.1",
        "barryvdh/laravel-dompdf": "^v0.9.0",
        "bensampo/laravel-enum": "^v3.0.0",
        "beyondcode/laravel-websockets": "^1.12.0",
        "doctrine/dbal": "^3.0.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.5",
        "jackiedo/dotenv-editor": "^1.2",
        "laravel-notification-channels/africastalking": "^2.1",
        "laravel/framework": "^8.40",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mailgun/mailgun-php": "^3.5",
        "prettus/l5-repository": "^2.6",
        "pusher/pusher-php-server": "^5.0.3",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-medialibrary": "^9.6",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-sluggable": "^2.2",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/breeze": "^v1.1",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
